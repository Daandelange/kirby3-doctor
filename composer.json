{
    "name": "bnomei/kirby3-doctor",
    "type": "plugin",
    "version": "1.0.0",
    "description": "Kirby 3 Plugin to check health of your CMS installation.",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "doctor",
        "check",
        "health",
        "audit",
        "security",
        "installation"
    ],
    "autoload": {
        "files": [
            "config.php"
        ],
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/k-next/kirby"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "getkirby/cms": "^3.0.0-RC-2.0",
        "sensiolabs/security-checker": "^5.0",
        "zendframework/zenddiagnostics": "^1.3"
    },
    "conflict": {
        "getkirby/kirby": "<3.0"
    },
    "suggests": {
        "bvdputte/kirby-log": "If installed results will be reporting to a log-file automatically.",
        "bnomei/kirby3-security-headers": "Easier Security Headers setup",
        "bnomei/kirby3-janitor": "Plugin which helps with clearing the Cache",
    },
    "scripts": {
        "zip": [
            "rm kirby3-doctor.zip",
            "composer install --no-dev",
            "composer remove getkirby/cms",
            "composer dumpautoload -o",
            "yarn",
            "yarn run build",
            "zip -r kirby3-doctor.zip . -x *.git* -x *.gif* -x 'node_modules/*'",
            "composer require getkirby/cms:'^3.0.0-RC-2.0'"
        ]
    }
}
